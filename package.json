    "morgan": "^1.10.0",
    "multer": "^1.4.4",
		"multer-s3": "2.10.0",
    "mysql2": "^3.4.3",
    "nodemailer": "^6.9.3",
    "sequelize": "^6.32.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
