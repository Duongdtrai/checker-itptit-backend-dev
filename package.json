    "aws-sdk": "^2.863.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "formidable": "^2.1.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
